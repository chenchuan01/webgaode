<script>
	var srcPath = '${ctx}/static/img/gallery/img/';
	jQuery(document).ready(function() {		
		App.setPage("simple_table");  //Set current page
		App.init(); //Initialise plugins and elements
		$('[data-toggle="tooltip"]').tooltip();
	});
	function lineBlowUp(from,to,pic){
		mapBox();
		$('#mapImg').attr('src',srcPath+pic);
		mapApp.nav_bus(from,to);
	};
	function mapBox(){
		$('#lineMap').toggle(500);
	}
</script>
